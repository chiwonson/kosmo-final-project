    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "TestApplication",
            "request": "launch",
            "mainClass": "BreadTour.TestApplication",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "BreadTourApplication",
            "request": "launch",
            "mainClass": "BreadTourApplication",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "BreadTourApplication",
            "request": "launch",
            "mainClass": "BreadTour.BreadTourApplication",
            "projectName": "demo"
        },
        {
            "name": "Flask Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/python/app/app.py",
            "console": "integratedTerminal",
            "env": {
                "FLASK_APP": "${workspaceFolder}/python/app/app.py",
                "FLASK_ENV": "development"
            }
        }
    ]
}